<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:c="using:WalletWasabi.Fluent.Controls"
             xmlns:address="clr-namespace:WalletWasabi.Fluent.ViewModels.Wallets.Receive.Address"
             xmlns:receive="clr-namespace:WalletWasabi.Fluent.Views.Wallets.Receive"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="address:SoftwareAddressViewModel"
             x:CompileBindings="True"
             x:Class="WalletWasabi.Fluent.Views.Wallets.Receive.Address.SoftwareAddressView">
  <c:ContentArea Title="Unused Receive Address"
                 EnableCancel="{Binding EnableCancel}"
                 EnableBack="{Binding EnableBack}"
                 EnableNext="True" NextContent="OK"
                 ScrollViewer.VerticalScrollBarVisibility="Disabled">
    <receive:ReceiveAddressView DataContext="{Binding ReceiveAddress}" />
  </c:ContentArea>
</UserControl>
